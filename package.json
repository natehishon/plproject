{
  "name": "plproj",
  "version": "1.0.0",
  "description": "",
  "main": "dist",
  "scripts": {
    "dev": "NODE_DEV=development nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "NOD_ENV=production pm2 start dist",
    "prestart": "npm run -s build",
    "lint": "eslint src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "author": "Nate Hish",
  "license": "ISC",
  "dependencies": {
    "@google/maps": "^0.5.5",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "bulma": "^0.7.2",
    "express": "^4.16.4",
    "json2csv": "^4.3.3",
    "mongoose": "^5.4.9"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^5.13.0",
    "nodemon": "^1.18.9"
  }
}
